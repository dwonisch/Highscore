                  assembly="HighScore.Data"
                  namespace="HighScore.Data">
  <class name="Player" table="players">
    <id name="Id">
      <generator class="identity" />
    </id>

    <property name="Name" />
    <property name="Female" />
    <property name="Child" />
  </class>
</hibernate-mapping>